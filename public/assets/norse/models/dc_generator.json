{
  "abbreviation": "dc",
  "elementType": "stimulator",
  "id": "dc_generator",
  "label": "direct current",
  "codeTemplate": "{{ view.label }} = torch.ones({{ params.timesteps.value }},{{ size }}) * {{ params.amplitude.value }}",
  "recordables": ["I"],
  "params": [
    {
      "id": "amplitude",
      "input": "valueSlider",
      "label": "amplitude of current",
      "max": 10,
      "min": -10,
      "step": 0.01,
      "unit": "pA",
      "value": 0
    },
    {
      "id": "timesteps",
      "input": "valueSlider",
      "label": "timesteps",
      "max": 100,
      "min": 0,
      "step": 1,
      "value": 0
    }
  ]
}
